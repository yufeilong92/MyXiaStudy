<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:paddingTop="15dp"
              android:paddingBottom="15dp"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_null"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/nothing"
        android:visibility="gone"
        android:gravity="center"
        android:textSize="@dimen/text_size_mediummin"
        android:textColor="@color/lawyer_list_text_tip2"
        />
    <LinearLayout
        android:id="@+id/ll_assign"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        android:paddingLeft="15dp"
        android:orientation="vertical"
        >
        <TextView
            android:id="@+id/tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2015-02-17"
            android:layout_marginBottom="5dp"
            android:textColor="@color/pack_up_text"
            android:textSize="@dimen/text_size_mediummin"
            />
        <TextView
            android:id="@+id/tv_1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/jamed_for_you_audit"
            android:layout_marginBottom="5dp"
            android:textStyle="bold"
            android:textColor="@color/lawyer_list_text_tip2"
            android:textSize="@dimen/text_size_mediummin"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tv_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/aid_audit_result"
                android:textColor="@color/grey_org"
                android:layout_marginBottom="5dp"
                android:textSize="@dimen/text_size_mediummin"
                />
            <TextView
                android:id="@+id/tv_22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text=""
                android:layout_marginLeft="5dp"
                android:textColor="@color/lawyer_list_text_tip2"
                android:textSize="@dimen/text_size_mediummin"
                />
        </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                >
                <TextView
                    android:id="@+id/tv_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/aid_audit_result_time"
                    android:textColor="@color/grey_org"
                    android:layout_marginBottom="5dp"
                    android:textSize="@dimen/text_size_mediummin"
                    />
                <TextView
                    android:id="@+id/tv_33"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:layout_marginLeft="5dp"
                    android:textColor="@color/lawyer_list_text_tip2"
                    android:textSize="@dimen/text_size_mediummin"
                    />
            </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            >
            <TextView
                android:id="@+id/tv_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/aid_audit_result_explain"
                android:textColor="@color/grey_org"
                android:layout_marginBottom="5dp"
                android:textSize="@dimen/text_size_mediummin"
                />
            <TextView
                android:id="@+id/tv_44"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="15dp"
                android:text=""
                android:layout_marginLeft="5dp"
                android:textColor="@color/lawyer_list_text_tip2"
                android:textSize="@dimen/text_size_mediummin"
                />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>